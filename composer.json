      "role": "Developer"
    },
    {
      "name": "Evgeniy Borisov",
      "homepage": "https://agel-nash.ru",
      "email": "laravel@agel-nash.ru",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/AgelxNash/laravel-request-logger",
  "support": {
    "email": "laravel@agel-nash.ru",
    "issues":"https://github.com/AgelxNash/laravel-request-logger/issues",
    "source":"https://github.com/AgelxNash/laravel-request-logger"
  },
  "require": {
    "php": ">=7.1.0",
    "illuminate/support": "~5.0",
    "psr/log": "~1.0",
    "symfony/http-foundation": "^4.1"
  },
  "autoload": {
    "psr-4": {
      "AgelxNash\\RequestLogger\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "laravel": {
      "providers": [
        "AgelxNash\\RequestLogger\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
